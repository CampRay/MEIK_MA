        <Table.Columns>
            <TableColumn Width="*"></TableColumn>
            <TableColumn Width="4*"></TableColumn>
            <TableColumn Width="*"></TableColumn>
        </Table.Columns>
        <TableRowGroup>
            <TableRow>                
                <TableCell RowSpan="4" Name="logoImg1">
                    <!--<TableCell.Background>
                        <ImageBrush ImageSource="/MEIKReport;component/Images/logo_k.png" Stretch="Uniform" AlignmentY="Top"/>
                    </TableCell.Background>-->
                </TableCell>
                <TableCell TextAlignment="Center" LineStackingStrategy="BlockLineHeight">
                    <Paragraph LineHeight="10"/>
                </TableCell>
                <TableCell></TableCell>
            </TableRow>
            <TableRow Name="titleImg1">
                <TableRow.Background>
                    <ImageBrush ImageSource="/MEIKReport;component/Images/title.png" AlignmentY="Top" Stretch="Uniform"></ImageBrush>
                </TableRow.Background>
                
                <TableCell TextAlignment="Center" LineStackingStrategy="BlockLineHeight">
                    <Paragraph LineHeight="32">                        
                    </Paragraph>
                </TableCell>
                <TableCell></TableCell>
            </TableRow>
            <TableRow>
                
                <TableCell TextAlignment="Center">
                    <Paragraph>
                        <TextBlock Text="快速乳房健康檢測報告" Foreground="#FF00657F" FontWeight="Bold" FontSize="28"></TextBlock>
                    </Paragraph>
                </TableCell>
                <TableCell></TableCell>
            </TableRow>
            <TableRow>
                
                <TableCell TextAlignment="Center">
                    <Paragraph>
                        <TextBlock Text="Rapid Breast Health Screening Report" Foreground="#FF00657F" FontWeight="Bold" FontSize="18"/>
                    </Paragraph>
                </TableCell>
                <TableCell></TableCell>
            </TableRow>            
        </TableRowGroup>
    </Table>
    <!--Personal info-->
    <Table Padding="50,0,50,0">        
        <TableRowGroup>
            <TableRow>
                <TableCell>
                    <Paragraph>
                        <Grid Width="692">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>                                
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="160"></ColumnDefinition>
                                <ColumnDefinition Width="3*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>                            
                            <Border BorderThickness="2,2,2,0" BorderBrush="#FF00657F" Background="#FF00657F"  CornerRadius="6,6,0,0" VerticalAlignment="Center">
                                <TextBlock Foreground="White" Background="#FF00657F" HorizontalAlignment="Left" VerticalAlignment="Center" Text="客戶資料 Client Information" Margin="5,2,5,2" FontSize="12"></TextBlock>
                            </Border>                            
                            <Border Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#FF00657F" Background="White">
                                <Grid Width="692" HorizontalAlignment="Left" Margin="10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <WrapPanel Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="客戶號碼 Client Number: "></TextBlock>
                                        <TextBlock Name="dataClientNum" Text="{Binding DataClientNum}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="姓名 Name: "></TextBlock>
                                        <TextBlock Name="dataName" Text="{Binding DataName}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="年齡 Age: "></TextBlock>
                                        <TextBlock Name="dataAge" Text="{Binding DataAge}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="香港身份證 HKID: "></TextBlock>
                                        <TextBlock Name="dataClientID" Text="{Binding DataClientID}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="身高 Height(cm): "></TextBlock>
                                        <TextBlock Name="dataHeight" Text="{Binding DataHeight}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="體重 Weight(kg): "></TextBlock>
                                        <TextBlock Name="dataWeight" Text="{Binding DataWeight}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="電話號碼 Mobile number: "></TextBlock>
                                        <TextBlock Name="dataMobile" Text="{Binding DataMobile}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="電郵 Email: "></TextBlock>
                                        <TextBlock Name="dataEmail" Text="{Binding DataEmail}"></TextBlock>
                                    </WrapPanel>
                                    <StackPanel Grid.Row="4" Grid.RowSpan="3" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="家族病歷 Family History: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="5" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Name="dataFamilyCancerDesc" Text="{Binding DataFamilyCancerDesc}" Margin="10,0,0,0"  FontSize="9" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                    <StackPanel Grid.Row="7" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="乳房健康狀況 Complaints: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="8" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Name="dataOtherDesc" Text="{Binding DataOtherDesc}" Margin="10,0,0,0" FontSize="9" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                    <StackPanel Grid.Row="10"  Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="月經狀況 Menstrual Condition: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="11" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Name="dataPostmenopauseDesc" Text="{Binding DataPostmenopauseDesc}" Margin="10,0,0,0" FontSize="9" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                    <StackPanel Grid.Row="13"  Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="產科 Obstetric: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="14" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Name="dataObstetricDesc" Text="{Binding DataObstetricDesc}" Margin="10,0,0,0" FontSize="9" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                    <StackPanel Grid.Row="16"  Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="病史 Anamnesis: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="17" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Name="dataGynecologicOtherDesc" Text="{Binding DataGynecologicOtherDesc}" Margin="10,0,0,0" FontSize="9" VerticalAlignment="Center" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                    <StackPanel Grid.Row="19" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        <TextBlock Text="檢查化驗 Examinations: "></TextBlock>                                        
                                    </StackPanel>
                                    <StackPanel Grid.Row="20" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Left" VerticalAlignment="Top">                                        
                                        <TextBlock Name="dataExaminationsOtherDesc" Text="{Binding DataExaminationsOtherDesc}" Margin="10,0,0,0" FontSize="9" VerticalAlignment="Center" TextWrapping="Wrap"></TextBlock>
                                    </StackPanel>
                                </Grid>
                            </Border>
                        </Grid>
                    </Paragraph>
                </TableCell>
            </TableRow>
            
        </TableRowGroup>
    </Table>
    <!--Screen info-->
    <Table Padding="50,0,50,0">
        <TableRowGroup>
            <TableRow>
                <TableCell>
                    <Paragraph>
                        <Grid Width="692">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="146"></ColumnDefinition>
                                <ColumnDefinition Width="3*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border BorderThickness="2,2,2,0" BorderBrush="#FF00657F" Background="#FF00657F"  CornerRadius="6,6,0,0" VerticalAlignment="Center">
                                <TextBlock Foreground="White" Background="#FF00657F" HorizontalAlignment="Left" VerticalAlignment="Center" Text="檢測記錄 Screen Record" Margin="5,2,5,2" FontSize="12"></TextBlock>
                            </Border>
                            <Border Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#FF00657F" Background="White">
                                <Grid Width="692" HorizontalAlignment="Left" Margin="10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>                                                                             
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <WrapPanel Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="檢測編號 Screen code: "></TextBlock>
                                        <TextBlock Name="dataUserCode" Text="{Binding DataUserCode}"/>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="檢測日期 Screen date: "></TextBlock>
                                        <TextBlock Name="dataScreenDate" Text="{Binding DataScreenDate}"/>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="檢測地點 Screen venue: "></TextBlock>
                                        <TextBlock Name="dataScreenLocation" Text="{Binding DataScreenLocation}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="檢測員姓名 Technician Name: "></TextBlock>
                                        <TextBlock Name="dataMeikTech" Text="{Binding Path=DataMeikTech}" ></TextBlock>
                                    </WrapPanel>                                    
                                </Grid>
                            </Border>
                        </Grid>
                    </Paragraph>
                </TableCell>
            </TableRow>
        </TableRowGroup>
    </Table>
    <!--Screen Result-->
    <Table Padding="50,0,50,0">
        <TableRowGroup>
            <TableRow>
                <TableCell>
                    <Paragraph>
                        <Grid Width="692">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="246"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="144"></ColumnDefinition>
                                <ColumnDefinition Width="3*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border BorderThickness="2,2,2,0" BorderBrush="#FF00657F" Background="#FF00657F"  CornerRadius="6,6,0,0" VerticalAlignment="Center">
                                <TextBlock Foreground="White" Text="檢測結果 Screen Result" Margin="5,2,5,2" FontSize="12" Background="#FF00657F" HorizontalAlignment="Left" VerticalAlignment="Center"></TextBlock>
                            </Border>
                            <Border Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#FF00657F" Background="White">
                                <Grid Width="692" HorizontalAlignment="Left" Margin="10,10,10,5">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="18"></RowDefinition>
                                        <RowDefinition Height="10"></RowDefinition>
                                        <RowDefinition Height="100"></RowDefinition>
                                        <RowDefinition Height="0"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="17"></RowDefinition>
                                        <RowDefinition Height="2"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="270"></ColumnDefinition>
                                        <ColumnDefinition Width="*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <WrapPanel Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="平均電導率 Mean Electrical Conductivity" Width="260" Foreground="#FF00597F"></TextBlock>
                                    </WrapPanel>
                                    <StackPanel Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="30,0,0,0">
                                        <WrapPanel Width="160" HorizontalAlignment="Center">
                                            <TextBlock Width="100" Text="左乳 Left Breast:"></TextBlock>
                                            <TextBlock Name="dataLeftMeanECOfLesion" Text="{Binding DataLeftMeanECOfLesion}" Margin="5,0,0,0"/>
                                        </WrapPanel>
                                        <StackPanel Height="20"></StackPanel>
                                        <WrapPanel Width="160" HorizontalAlignment="Center">
                                            <TextBlock Width="100" Text="右乳 Right Breast:"></TextBlock>
                                            <TextBlock Name="dataRightMeanECOfLesion" Text="{Binding DataRightMeanECOfLesion}" Margin="5,0,0,0"/>
                                        </WrapPanel>
                                    </StackPanel>
                                    <Grid Grid.Row="0" Grid.Column="1" Grid.RowSpan="3" HorizontalAlignment="Center">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="1.1*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>
                                            <RowDefinition Height="*"/>                                            
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="80"/>
                                            <ColumnDefinition Width="260"/>
                                        </Grid.ColumnDefinitions>
                                        <Border  Grid.Row="0" Grid.Column="0" BorderBrush="Black" BorderThickness="1,1,1,1" Background="#FF35A2B4">
                                            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <TextBlock Text="年齡" FontSize="6" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                                                <TextBlock Text="Age" FontSize="6" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                                            </StackPanel>
                                        </Border>
                                        <Border Grid.Row="0" Grid.Column="1" BorderBrush="Black" BorderThickness="0,1,1,1" Background="#FF35A2B4">
                                            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                                <TextBlock Text="平均電導率正常範圍" FontSize="6" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                                                <TextBlock Text="*Mean Electrical Conductivity Normal Range" FontSize="6" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                                            </StackPanel>
                                        </Border>
                                        <Border  Grid.Row="1" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1" VerticalAlignment="Center">
                                            <Label Content="&lt;29" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="1" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1" VerticalAlignment="Center">
                                            <Label Content="0.18 - 0.44" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="2" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1">
                                            <Label Content="30-39" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="2" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1">
                                            <Label Content="0.16 - 0.53" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="3" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1">
                                            <Label Content="40-49" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="3" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1">
                                            <Label Content="0.22 - 0.63" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="4" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1">
                                            <Label Content="50-59" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="4" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1">
                                            <Label Content="0.32 - 0.72" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="5" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1">
                                            <Label Content="60-69" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="5" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1">
                                            <Label Content="0.43 - 0.78" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="6" Grid.Column="0" BorderBrush="Black" BorderThickness="1,0,1,1">
                                            <Label Content="&gt;70" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                        <Border  Grid.Row="6" Grid.Column="1" BorderBrush="Black" BorderThickness="0,0,1,1">
                                            <Label Content="0.50 - 0.64" FontSize="6" HorizontalAlignment="Center" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"></Label>
                                        </Border>
                                    </Grid>

                                    <WrapPanel Grid.Row="4" Grid.Column="0" Grid.RowSpan="2" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="相對電導率 Comparative Electrical Conductivity"  Foreground="#FF00597F"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="4" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Text="1 phase(-0.07 - 0.07)" Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftComparativeElectricalConductivity1" Text="{Binding DataLeftComparativeElectricalConductivity1}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="5" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Text="2 phase(-0.10 - 0.10)" Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftComparativeElectricalConductivity2" Text="{Binding DataLeftComparativeElectricalConductivity2}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="6" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Name="dataComparativeElectricalConductivity3" Text="{Binding DataMeanElectricalConductivity3}" Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftComparativeElectricalConductivity3" Text="{Binding DataLeftComparativeElectricalConductivity3}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="7" Grid.Column="0" Grid.RowSpan="2" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="左右側乳腺差異 Divergence Between Histograms"  Foreground="#FF00597F"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="7" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Text="1 phase(below 40%)" Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftDivergenceBetweenHistograms1" Text="{Binding DataLeftDivergenceBetweenHistograms1}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="8" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Text="2 phase(below 40%)" Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftDivergenceBetweenHistograms2" Text="{Binding DataLeftDivergenceBetweenHistograms2}"></TextBlock>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="9" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="50,0,0,0">
                                        <TextBlock Name="dataDivergenceBetweenHistograms3" Text="{Binding DataMeanElectricalConductivity3}"  Width="200" ></TextBlock>
                                        <TextBlock Name="dataLeftDivergenceBetweenHistograms3" Text="{Binding DataLeftDivergenceBetweenHistograms3}"></TextBlock>
                                    </WrapPanel>
                                </Grid>
                            </Border>

                        </Grid>
                    </Paragraph>
                </TableCell>
            </TableRow>

        </TableRowGroup>
    </Table>
    <!--Result-->
    <Table Padding="50,0,50,0">
        <TableRowGroup>
            <TableRow>
                <TableCell>
                    <Paragraph>
                        <Grid Width="692">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"></RowDefinition>
                                <RowDefinition Height="99"></RowDefinition>                                
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="118"></ColumnDefinition>
                                <ColumnDefinition Width="3*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border BorderThickness="2,2,2,0" BorderBrush="#FF00657F" Background="#FF00657F"  CornerRadius="6,6,0,0" VerticalAlignment="Center">
                                <TextBlock Foreground="White" Background="#FF00657F" HorizontalAlignment="Left" VerticalAlignment="Center" Text="總覽 Overview" Margin="5,2,5,2" FontSize="12"></TextBlock>
                            </Border>
                            <Border Grid.Row="1" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#FF00657F" Background="White">
                                <Grid Width="692" HorizontalAlignment="Left" Margin="10,10,10,3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="1"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                        <RowDefinition Height="1"></RowDefinition>
                                        <RowDefinition Height="16"></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <WrapPanel Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="BI-EIM 分數 Bi-EIM Points: "></TextBlock>
                                        <TextBlock Name="dataTotalPts" Text="{Binding DataTotalPts}"/>
                                    </WrapPanel>
                                    <WrapPanel Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="結果分析 Result Analysis: " Width="140"></TextBlock>
                                        <TextBlock Name="dataConclusion" Text="{Binding DataConclusion}"/>
                                    </WrapPanel>

                                    <WrapPanel Grid.Row="2" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <TextBlock Text="建議覆檢 Suggestion Re-examination: " Width="205"></TextBlock>
                                        <TextBlock Name="dataRecommendation" Text="{Binding DataRecommendation}"/>
                                    </WrapPanel>
